#
    Stack programming language
    log output library
#

[] (log-list) var

( #output information log#
    (msg) var
    ([INFO] msg)
    (msg) msg replace
    copy print 
    log-list swap append
    (log-list) var
    (msg) free
) (log-info) var

( #output warning log#
    (msg) var
    ([!WARNING!] msg)
    (msg) msg replace
    copy print 
    log-list swap append
    (log-list) var
    (msg) free
) (log-warn) var


( (name) var
    log-list name write-file
) (save) var