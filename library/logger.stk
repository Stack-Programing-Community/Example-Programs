#--------------------+
| Stackプログラミング言語 |
| ログ表示ライブラリ      |
+--------------------#

[] (log-list) var

( #情報ログを出力#
    (msg) var
    ([INFO] msg)
    (msg) msg replace
    copy print 
    log-list swap append
    (log-list) var
    (msg) free
) (log-info) var

( #警告ログを出力#
    (msg) var
    ([!WARNING!] msg)
    (msg) msg replace
    copy print 
    log-list swap append
    (log-list) var
    (msg) free
) (log-warn) var


( (name) var
    log-list name write-file
) (save) var